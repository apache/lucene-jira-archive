From dae153f4cdc9e9e265f26cfd8ccfda1ed013c5a8 Mon Sep 17 00:00:00 2001
From: Namgyu Kim <kng0828@gmail.com>
Date: Thu, 6 Sep 2018 02:32:32 +0900
Subject: [PATCH] LUCENE-8487: Code Optimizations in FieldType

Signed-off-by: Namgyu Kim <kng0828@gmail.com>
---
 .../src/java/org/apache/lucene/document/FieldType.java     | 7 ++-----
 1 file changed, 2 insertions(+), 5 deletions(-)

diff --git a/lucene/core/src/java/org/apache/lucene/document/FieldType.java b/lucene/core/src/java/org/apache/lucene/document/FieldType.java
index 3c7d276e70..068dc97a0f 100644
--- a/lucene/core/src/java/org/apache/lucene/document/FieldType.java
+++ b/lucene/core/src/java/org/apache/lucene/document/FieldType.java
@@ -296,9 +296,7 @@ public class FieldType implements IndexableFieldType  {
         throw new IllegalArgumentException("when dimensionCount is 0, dimensionNumBytes must 0; got " + dimensionNumBytes);
       }
     } else if (dimensionNumBytes == 0) {
-      if (dimensionCount != 0) {
-        throw new IllegalArgumentException("when dimensionNumBytes is 0, dimensionCount must 0; got " + dimensionCount);
-      }
+      throw new IllegalArgumentException("when dimensionNumBytes is 0, dimensionCount must 0; got " + dimensionCount);
     }
 
     this.dimensionCount = dimensionCount;
@@ -429,7 +427,6 @@ public class FieldType implements IndexableFieldType  {
     if (storeTermVectorPositions != other.storeTermVectorPositions) return false;
     if (storeTermVectors != other.storeTermVectors) return false;
     if (stored != other.stored) return false;
-    if (tokenized != other.tokenized) return false;
-    return true;
+    return tokenized == other.tokenized;
   }
 }
