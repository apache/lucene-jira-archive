+package org.apache.lucene.analysis.opennlp;
+
+import org.apache.lucene.analysis.TokenStream;
+import org.apache.lucene.analysis.util.TokenFilterFactory;
+
+import java.nio.charset.StandardCharsets;
+import java.util.Map;
+import java.util.Set;
+
+/**
+ * Factory for {@link FilterPayloadsFilter}.
+ * <pre class="prettyprint" >
+ * &lt;fieldType name="text_stop" class="solr.TextField" positionIncrementGap="100" autoGeneratePhraseQueries="true"&gt;
+ *   &lt;analyzer&gt;
+ *     &lt;tokenizer class="solr.WhitespaceTokenizerFactory"/&gt;
+ *     &lt;filter class="solr.FilterPayloadsFilterFactory"
+ *             payloadList="NN,JJ"
+ *             keepPayloads="true"
+ *     &lt;/filter&gt;
+ *   &lt;/analyzer&gt;
+ * &lt;/fieldType&gt;</pre>
+ * @version $Id$
+ */
+public class FilterPayloadsFilterFactory extends TokenFilterFactory {
+  final public static String PAYLOAD_LIST = "payloadList";
+  final public static String KEEP_PAYLOADS = "keepPayloads";
+
+  private final byte[][] payloads;
+  private final boolean keepTokens;
+
+  public FilterPayloadsFilterFactory(Map<String,String> args) {
+    super(args);
+    Set<String> payloadList = getSet(args, PAYLOAD_LIST);
+    payloads = payloadList == null ? new byte[0][]
+        : (byte[][])payloadList.stream().map(p -> p.getBytes(StandardCharsets.UTF_8)).toArray();
+    keepTokens = getBoolean(args, KEEP_PAYLOADS, true);
+    if (!args.isEmpty()) {
+      throw new IllegalArgumentException("Unknown parameters: " + args);
+    }
+  }
+
+  public FilterPayloadsFilter create(TokenStream input) {
+    return new FilterPayloadsFilter(input, payloads, keepTokens);
+  }
+}
