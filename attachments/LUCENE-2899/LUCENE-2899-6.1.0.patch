+
+import org.apache.lucene.analysis.TokenFilter;
+import org.apache.lucene.analysis.TokenStream;
+import org.apache.lucene.analysis.tokenattributes.PayloadAttribute;
+
+/**
+ * Remove all payloads from all tokens
+ */
+public final class StripPayloadsFilter extends TokenFilter {
+  
+  public StripPayloadsFilter(TokenStream input) {
+    super(input);
+  }
+  
+  @Override
+  public boolean incrementToken() throws IOException {
+    if (input.incrementToken()) {
+     PayloadAttribute payloadAtt = getAttribute(PayloadAttribute.class);
+      if (payloadAtt != null) {
+        payloadAtt.setPayload(null);
+      }
+      return true;
+    }
+    return false;
+  }
+}
