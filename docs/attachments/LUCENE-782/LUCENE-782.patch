+  JField = org.apache.lucene.document.Field
+
+  class JField
+
+    def flags
+      str = ""
+      ( not is_indexed ) && str += "i"
+      is_compressed && str += "C"
+      is_lazy && str += "L"
+      ( not is_tokenized ) && str += "t"
+      get_omit_norms && str += "O"
+      str
+    end
+
+    def value
+      string_value
+    end
+
+  end
+
+  class Document
+
+    attr_accessor :id
+    attr_accessor :score
+
+    class Fields
+      
+      def hash
+        @hash = {}
+
+        @jdoc.get_fields.each do |field|
+          @hash[ field.name ] = field
+        end
+      end
+
+      def initialize jdoc
+        @jdoc = jdoc
+        hash
+      end
+
+      def [] key
+        @hash[ key ]
+        @jdoc.get_fields( key )
+      end
+
+      def each
+        @jdoc.get_fields.each { |f| yield f }
+      end
+
+    end
+
+    def construct list, java
+      @java = java || JDocument.new
+      if list
+        pair_up list  do |k,v|
+          doc.add( JField.new( k.to_s, StringReader.new( v ) ) )
+        end
+      end
+      self
+    end
+
+    def self.from_java( java )
+      doc = new
+      doc.construct nil, java
+    end
+
+    def initialize( list = nil )
+      construct list, nil
+    end
+
+    def self.pair_up list
+      ( 0...list.length-1 ).step(2) do |i|
+        k,vs = list[i,2]
+        vs.to_a.each { |v| yield [k, v] }
+      end
+    end
+
+    def fields
+      Fields.new( @java )
+    end
+
+  end
+
+end
